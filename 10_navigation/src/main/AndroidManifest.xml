<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huahen.navigation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".basicJump.BasicJumpActivity" />
        <activity android:name=".safeArgs.SafeArgsActivity" />
        <activity android:name=".navui.NavUiActivity" />
        <activity android:name=".navui.menu.MenuActivity" />
        <activity android:name=".navui.drawerlayout.DrawerLayoutActivity" />
        <activity android:name=".navui.bottomnavigationbar.BottomNavigationBarActivity" />
        <activity android:name=".deepLink.DeepLinkActivity">
            <nav-graph android:value="@navigation/nav_deeplink" />
        </activity>
        <activity android:name=".viewModel.ViewModelActivity" />
    </application>

</manifest>